{
    "name": "${localWorkspaceFolderBasename}",
    "dockerComposeFile": "../docker-compose.yml",
    "service": "rstudio",
    "remoteUser": "rstudio",
    "extensions": [
		"REditorSupport.r",
	],
    
	"forwardPorts": [8080, 8787],
    "workspaceFolder": "/home/rstudio/src",
    "shutdownAction": "stopCompose",
}
